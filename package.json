{
	"name": "polycomment",
	"displayName": "PolyComment",
	"description": "Translate code comments and README files into multiple languages using lingo.dev",
	"version": "0.0.1",
	"publisher": "BhupendraLute",
	"repository": {
		"type": "git",
		"url": "https://github.com/BhupendraLute/PolyComment"
	},
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.85.0"
	},
	"files": [
		"out",
		"LICENSE.md",
		"README.md"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "polycomment.translateComments",
				"title": "PolyComment: Translate Comments"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.85.0",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8.54.0",
		"glob": "^10.3.10",
		"mocha": "^10.2.0",
		"typescript": "^5.3.2",
		"ts-loader": "^9.5.1",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"@vscode/test-electron": "^2.3.8"
	},
	"dependencies": {
		"typescript": "^5.3.3",
		"remark-parse": "^11.0.0",
		"unified": "^11.0.4",
		"unist-util-visit": "^5.0.0",
		"@lingo.dev/_sdk": "latest"
	}
}
